{
    "package": {
        "subject": "l10178",
        "repo": "maven",
        "name": "${project.artifactId}",
        "desc": "A collection of IP and MAC utilities.",
        "website_url": "https://github.com/l10178/grapes-lang",
        "issue_tracker_url": "https://github.com/l10178/grapes-lang/issues",
        "vcs_url": "https://github.com/l10178/grapes-lang.git",
        "licenses": [
            "MIT"
        ],
        "labels": [
            "IP",
            "MAC"
        ],
        "public_download_numbers": true,
        "public_stats": true
    },
    "version": {
        "name": "${project.version}",
        "released": "${timestamp}",
        "vcs_tag": "${project.version}"
    },
    "files": [
        {
            "includePattern": "${project.build.directory}/(.*\\.jar)",
            "uploadPattern": "com/nxest/${project.artifactId}/${project.version}/$1"
        },
        {
            "includePattern": "${project.basedir}/pom.xml",
            "uploadPattern": "com/nxest/${project.artifactId}/${project.version}/${project.artifactId}-${project.version}.pom"
        }
    ],
    "publish": true
}
