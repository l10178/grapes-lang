{
    "package": {
        "subject": "l10178",
        "repo": "oss-repo",
        "name": "@project.artifactId@",
        "desc": "A collection of IP and MAC utilities.",
        "website_url": "https://github.com/l10178/grapes-lang",
        "issue_tracker_url": "https://github.com/l10178/grapes-lang/issues",
        "vcs_url": "https://github.com/l10178/grapes-lang.git",
        "licenses": [
            "MIT"
        ],
        "labels": [
            "IP",
            "MAC"
        ],
        "public_download_numbers": true,
        "public_stats": true
    },
    "version": {
        "name": "@version@",
        "released": "@release_date@",
        "vcs_tag": "@version@"
    },
    "files": [
        {
            "includePattern": "target/(.*\\.jar)",
            "uploadPattern": "com/github/tomschi/${project.artifactId}/${project.version}/$1"
        },
        {
            "includePattern": "pom.xml",
            "uploadPattern": "com/github/tomschi/${project.artifactId}/${project.version}/${project.artifactId}-${project.version}.pom"
        }
    ],
    "publish": true
}
